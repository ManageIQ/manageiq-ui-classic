/ begin: app/views/cloud_object_store_object/show.html.haml
- if @display == 'clouds'
  = render :partial => "layouts/gtl", :locals => {:action_url => "@lastaction"}
- else
  - if @showtype == "item"
    = render :partial => "layouts/item"
  - elsif @showtype == "main"
    = render :partial => "layouts/textual_groups_generic"
/ end: app/views/cloud_object_store_object/show.html.haml
