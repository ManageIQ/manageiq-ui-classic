/ begin: app/views/static/deploy_containers_provider/deploy-provider-details-general-review.html.haml
%div{"ng-controller" => "containers.deployProviderReviewController"}
  %form.form
    .wizard-miq-review-item
      %span.wizard-miq-review-item-label
        = _("Name:")
      %span.wizard-miq-review-item-value {{data.providerName}}
    .wizard-miq-review-item
      %span.wizard-miq-review-item-label
        = _("Type:")
      %span.wizard-miq-review-item-value {{getProviderType()}}
/ end: app/views/static/deploy_containers_provider/deploy-provider-details-general-review.html.haml
