/ begin: app/views/ops/_diagnostics_timelines_tab.html.haml
- if @sb[:active_tab] == "diagnostics_timelines"
  = render :partial => "layouts/flash_msg"
  - if @tl_record
    :javascript
      // Create from/to date JS vars to limit calendar selection range
      ManageIQ.calendar.calDateFrom = new Date(#{@tl_options.date.start});
      ManageIQ.calendar.calDateTo = new Date(#{@tl_options.date.end});
    = render :partial => 'layouts/tl_options'
    = render :partial => 'layouts/tl_detail'
  - else
    = render :partial => 'layouts/info_msg', :locals => {:message => _("This Server's Virtual Machine has not been identified.")}
  %div{:style => "padding-bottom: 20px"}
/ end: app/views/ops/_diagnostics_timelines_tab.html.haml
