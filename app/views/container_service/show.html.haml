/ begin: app/views/container_service/show.html.haml
- if %w( container_routes container_services container_groups container_nodes).include?(@display)
  = render :partial => "layouts/gtl", :locals => {:action_url => "show/#{@record.id}"}
- elsif @showtype == "main"
  = render :partial => "layouts/textual_groups_generic"
- elsif @showtype == "performance"
  = render(:partial => "layouts/performance_async")

/ end: app/views/container_service/show.html.haml
